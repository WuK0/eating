page{
  background-color: #FFFFCC
}
.box{
  width: 90%;
  height: 125rpx;
  margin: 70rpx auto;
  text-align: center;
  color: rgb(65, 53, 53);
  background-color: #FFFF99;
  border-radius: 0 12px 12px;
}
.box navigator {
  width:100%;
  height: 100%;
  display: flex;
}

.box_img {
  width: 125rpx;
  height: 125rpx;
  margin-left: 35rpx;
  align-items: center;
  display: flex;
  justify-content: center;
}
.box_img image {
  width: 110rpx;
  height: 110rpx;
}
.box_text {
  flex: 1;
  height: 125rpx;
  line-height: 125rpx;
  font-family: 'Roboto'; 
  text-align: left;
  font-weight: 700;
  margin-left: 140rpx;
  font-size: 1.25rem;
}

