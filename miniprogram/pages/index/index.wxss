/**index.wxss**/
page{
  background-color: #FFFFCC;
}
.box {
  width: 90%;
  height: 350rpx;
  display: flex;
  margin: 2.5% auto;
  justify-content: center; 
  align-items: center;
}
.left{
  margin-right: 5%; 
}
.left,.right {
  display: flex;
  align-items: center;
  justify-content: center;  
  flex-grow: 1;
  background-color: #FFFF99;
  padding: 35rpx 0;
  text-align: center;
}
.left navigator,.right navigator{
  height: 100%;
  width: 100%;
}
.left text,.right text{
  padding-top: 15rpx;
  font-weight: 700;
}
.img_box {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}
.left image,.right image {
  width: 200rpx;
  height: 200rpx;
}
